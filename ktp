#Program KTP
nik =input("Masukkan No NIK:")
nama =input("Masukkan Nama:")
tempat =input("Masukkan Tempat Lahir :")
tanggal_lahir =input("Masukkan Tanggal Lahir:")
jenis_kelamin =input("Masukkan Jenis Kelamin :")
alamat =input("Masukkan Alamat :")
RT =input("Masukkan RT:")
RW =input("Masukkan RW:")
kel =input("Masukkan Kelurahan:")
desa =input("Masukkan Desa:")
kecamatan=input("Masukkan Kecamatan:")
agama =input('Masukkan Agama:')
status_perkawinan =input("Masukkan Status Perkawinan:")
pekerjaan =input("Masukkan Pekerjaan:")
kewarganegaraan =input("Masukkan Kewarganegaraan:")
berlaku_hingga =input("Berlaku Hingga:")

print("NIK		:{0}".format(nik))
print("Nama			:{0}".format(nama))
print("Tempat/Tanggal lahir 	:{0}/{1}".format(tempat,tanggal_lahir))
print('Jenis Kelamin        	:{0}'.format(jenis_kelamin))
print("Alamat               	:{0}".format(alamat))
print("      RT/RW          	:{0}/{1}".format(RT,RW))
print("      Kel/Desa       	:{0}/{1}".format(kel,desa))
print("      Kecamatan      	:{0}".format(kecamatan))
print("Agama                	:{0}".format(agama))
print("Status Perkawinan    	:{0}".format(status_perkawinan))
print("Pekerjaan           	:{0}".format(pekerjaan))
print("Kewarganegaraan      	:{0}".format(kewarganegaraan))
print("Berlaku Hingga       	:{0}".format(berlaku_hingga))
